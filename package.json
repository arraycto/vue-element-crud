{
  "name": "vue-element-crud",
  "version": "3.1.0",
  "description": "A CRUD component for vue projects to create, retrieve, update and destroy models.",
  "main": "dist/crud.common.js",
  "directories": {
    "lib": "dist",
    "example": "example"
  },
  "scripts": {
    "dev": "npm run dev:inline01",
    "dev:inline01": "cross-env NODE_ENV=development webpack-dev-server --config \"./examples/customer-dialog-form/webpack.config.js\" --open --hot",
    "dev:inline02": "cross-env NODE_ENV=development webpack-dev-server --config \"./examples/customer-inline-form/webpack.config.js\" --open --hot",
    "demo:inline02": "cross-env NODE_ENV=production webpack --config \"./example/customer-inline-form/webpack.config.js\"",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build:watch": "cross-env NODE_ENV=production webpack --progress --hide-modules -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wisetc/vue-element-crud.git"
  },
  "keywords": [
    "vue-crud",
    "vue-element-crud",
    "element-crud"
  ],
  "author": {
    "name": "Mark Wang",
    "email": "zhi@uqugu.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wisetc/vue-element-crud/issues"
  },
  "homepage": "https://github.com/wisetc/vue-element-crud#readme",
  "dependencies": {
    "element-ui": "^2.2.0",
    "vue": "2.6.10",
    "vue-class-component": "^7.1.0"
  },
  "devDependencies": {
    "vue-template-compiler": "2.6.10",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "i18n-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "style-loader": "^0.19.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "vue-loader": "^15.7.0",
    "webpack": ">=4 < 4.29",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.1.4"
  }
}
